  "eslint.useFlatConfig": true,
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": "explicit"
  },
  "editor.formatOnSave": true,
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "oxc.enable": true,
  "cSpell.words": [
    "commitlint",
    "interceptable",
    "oxlint",
    "Pavel",
    "pointerdown",
    "Renderable",
    "sandboxing",
    "skywarth",
    "tupple",
    "vite",
    "Voronin",
    "XYWH"
  ]
}
